{
  "manifest_version": 3,
  "name": "CF Navigator",
  "description": "Navigate contest problems from same page.",
  "version": "0.0.1",
  "icons": {
    "16": "/images/icon 16x16.png",
    "48": "/images/icon 48x48.png",
    "128": "/images/icon 128x128.png"
  },
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "/template/popup.html",
    "default_icon": {
      "16": "/images/icon 16x16.png",
      "48": "/images/icon 48x48.png",
      "128": "/images/icon 128x128.png"
    }
  }
}